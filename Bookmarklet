javascript:(async()=>{const t=prompt("Text to type");if(!t)return;const e=document.activeElement;if(!e||!(e.isContentEditable||"value"in e)){alert("Click inside a text field first.");return;}const s=m=>new Promise(r=>setTimeout(r,m));for(const c of t){if(e.isContentEditable){document.execCommand("insertText",false,c);}else{e.value+=c;e.dispatchEvent(new InputEvent("input",{bubbles:true}));}await s(60+Math.random()*140);}})();
